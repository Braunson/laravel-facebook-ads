{
    "name": "edbizarro/laravel-facebook-ads",
    "type": "library",
    "description": "Facebook Ads for Laravel 5",
    "license": "MIT",
    "keywords": [
        "laravel",
        "facebook",
        "ads",
        "sdk",
        "php"
    ],
    "authors": [
        {
            "name": "Eduardo Bizarro",
            "email": "edbizarro@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "facebook/php-ads-sdk": "^2.6",
        "codacy/coverage": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "fabpot/php-cs-fixer": "^1.11",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0",
        "orchestra/testbench": "^3.2",
        "mockery/mockery": "^0.9.4",
        "symfony/console": "~2.5"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Edbizarro\\LaravelFacebookAds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFacebookAds\\Test\\": "tests/"
        }
    },
    "config":{
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
